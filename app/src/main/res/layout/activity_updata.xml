<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.drop.spper.mvp.ui.activity.FeedBackActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/background" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <include
            android:id="@+id/include6"
            layout="@layout/mytoolbar" />


        <ImageView
            android:layout_width="350px"
            android:layout_height="350px"
            android:src="@drawable/icon"
            android:layout_below="@+id/include6"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="150px"
            android:id="@+id/imageView7" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="200px"
            android:layout_below="@+id/imageView7"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="200px">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dedede"
                android:id="@+id/linearLayout7" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="当前版本"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:layout_below="@+id/linearLayout7"
                android:layout_alignParentStart="true"
               android:layout_margin="30px"
                android:id="@+id/textView7" />

            <TextView
                android:id="@+id/tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.0"
                android:textSize="20sp"
                android:layout_margin="30px"
                android:layout_alignParentEnd="true"
                android:textColor="@color/white"/>

            <LinearLayout
                android:layout_below="@id/tv"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10px"
                android:background="#dedede"
                android:id="@+id/linearLayou87" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="版本更新"
                android:textSize="20sp"
                android:layout_margin="30px"
                android:textColor="@color/white"
                android:layout_below="@+id/linearLayou87"
                 />

            <TextView
                android:id="@+id/tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="已是最新版本"
                android:textSize="20sp"
                android:layout_alignParentEnd="true"
                android:textColor="@color/white"
                android:layout_margin="30px"
                android:layout_below="@+id/textView7" />

            <LinearLayout
                android:layout_below="@id/tv1"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10px"
                android:background="#dedede"
                android:id="@+id/linearLayou88" />

        </com.zhy.autolayout.AutoRelativeLayout>


    </com.zhy.autolayout.AutoRelativeLayout>


</com.zhy.autolayout.AutoRelativeLayout>